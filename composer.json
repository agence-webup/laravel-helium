{
    "name": "webup/laravel-helium",
    "description": "Laravel package for agence-webup/helium.",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Ducatel",
            "email": "baptiste@agence-webup.com"
        },
        {
            "name": "Bryan Levieux",
            "email": "bryan@agence-webup.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^7.3|^8.0",
        "anlutro/l4-settings": ">=0.7.3",
        "webup/laravel-form": "1.*",
        "yajra/laravel-datatables-oracle": "9.*",
        "laravel/ui": "^2.0|^3.0",
        "laravel/helpers": "^1.2",
        "spatie/laravel-permission": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Webup\\LaravelHelium\\": "src/"
        },
        "files": [
            "./src/Core/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webup\\LaravelHelium\\Core\\CoreServiceProvider"
            ],
            "aliases": {
                "Helium": "Webup\\LaravelHelium\\Core\\Facades\\Helium"
            }
        }
    },
    "minimum-stability": "dev"
}
